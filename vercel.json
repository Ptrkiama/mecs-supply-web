{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "mecsgeneralsupply.vercel.app"
        }
      ],
      "destination": "https://mecgeneralsupply.co.tz/$1",
      "permanent": true
    }
  ]
}
{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
